string numb(int b, int c)
{
	if (b == 0)
	{
		return "";
	}
	if (b == 1)
	{
		if (c == 0)
		{
			return  "ten";
		}
		if (c == 1)
		{
			return  "eleven";
		}
		if (c == 2)
		{
			return  "twelve";
		}
		if (c == 3)
		{
			return  "thirteen";
		}
