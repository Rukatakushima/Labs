	}

}

string hundr(int a, int c, int b)
{
	if ((b != 0) or (c != 0))
	{
		return "hundred and ";
	}
	else
	{
		return "hundred";
	}
}


int main()
{
	int x, a, b, c;
	cout << "Write an integer in the range 100-999" << endl;
	cin >> x;
	if (x < 100 || x>999) {
		cout << "An integer should be in the range 100-999" << endl;
		return 0;
	}
	a = x / 100;
	b = x % 10;
	c = (x / 10) % 10;
	if (c==0) {
		cout << digit(a) << hundr(a, c, b) << digit(b) << endl;
		return 0;
	}
	if (c == 1) {
		cout << digit(a) << hundr(a, c, b) << numb(c, b) << endl;
		return 0;
	}
	cout <<  digit(a) << hundr(a, c, b) << numb(c, b) << digit(b) <<  endl;
	return 0;
}
